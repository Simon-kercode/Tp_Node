<!DOCTYPE html>
<html lang="fr">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Panier</title>
    <link rel="stylesheet" href="./css/index.css">
</head>

<body>
    <% users.forEach(user => { %>
        <h1>
            Compte numéro : <%= user.nom %> <%= user.prenom %>
        </h1>
        <p>
            Votre email : <%= user.mail %> 
        </p>
    <% }); %>
    
    <% produits.forEach(produit => { %>
        <h1>
            Compte numéro : <%= produit.nom %> <%= produit.prix %>
        </h1>
    <% }); %>
</body>

</html>